        <activity android:name=".User.aj_UserLogin" />
        <activity android:name=".User.aj_UserSignUp" />
        <activity android:name=".Admin.aj_admin_signUp" />
        <activity android:name=".aj_SignUpType" />
        <activity android:name=".Admin.aj_AdminLogin" />
        <activity android:name=".User.aj_UserHomePage" />

        <service android:name=".aj_NotificationService">
            <intent-filter>
                <action android:name="com.google.firebase.MESSAGING_EVENT" />
            </intent-filter>
        </service>

        <activity android:name=".User.Show_User_details" />
        <activity android:name=".aj_User_reply_TO_Admin" />
        <activity android:name=".aj_Set_photogalarry" />
